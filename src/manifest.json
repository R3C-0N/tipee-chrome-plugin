{
  "manifest_version": 3,
  "name": "tipee validateur",
  "description": "entrez et sortez en un clic",
  "version": "0.2",
  "icons": {
    "128": "img/128.png"
  },
  "action": {
    "default_title": "Tipee",
    "default_icon": "img/128.png",
    "default_popup": "tpl/application.html"
  },
  "host_permissions": [
    "*://*.mathis-medard.com/*"
  ],
  "permissions": [
    "storage",
    "unlimitedStorage",
    "notifications",
    "cookies"
  ]
}
